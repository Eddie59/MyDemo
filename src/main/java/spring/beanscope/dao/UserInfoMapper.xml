<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="spring.beanscope.dao.UserInfoMapper">
    <select id="getUserInfo" resultType="spring.beanscope.model.UserInfo">
        select *
        from `userinfo`
